{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/frontend/src",
  "projectType": "application",
  "tags": ["type:service", "scope:frontend"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd services/frontend && npm run build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd services/frontend && npm run dev"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd services/frontend && npm run lint"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd services/frontend && npm test"
      }
    }
  }
}